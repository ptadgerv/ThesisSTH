@article{abelEstimatingPrevalenceInfectious1999,
  title = {Estimating the Prevalence of Infectious Agents Using Pooled Samples: {{Biometrical}} Considerations},
  shorttitle = {Estimating the Prevalence of Infectious Agents Using Pooled Samples},
  author = {Abel, Ulrich and Schosser, Rudolf and S{\"u}ss, Jochen},
  year = {1999},
  month = dec,
  journal = {Zentralblatt f\"ur Bakteriologie},
  volume = {289},
  number = {5},
  pages = {550--563},
  issn = {0934-8840},
  doi = {10.1016/S0934-8840(99)80009-7},
  urldate = {2023-05-20},
  abstract = {Pooled testing of units is a common approach in the prevalence estimation of infectious agents, which leads to a reduction of total costs of diagnostic testing. We examine how the pool size affects the statistical properties of the prevalence estimator r{\fontencoding{LECO}\selectfont\char177}. Exact formulae are used to determine bias and precision of r{\fontencoding{LECO}\selectfont\char177}. It is shown that with moderate pool sizes the (upward) bias of r{\fontencoding{LECO}\selectfont\char177} is negligible. If there is no diagnostic error, the random error of increases slightly with higher pool sizes, whereas if sensitivity and specificity are lower than 1, pooling may markedly decrease the random error of r{\fontencoding{LECO}\selectfont\char177}. Another reason why pooling may be beneficial (and even indispensable) is that it greatly reduces the huge bias that can result if the assumed values of the sensitivity and specificity of the diagnostic test are not equal to the true values. The numerical calculations show that, in case of prevalence rates of up to 5\% and total sample sizes of n{$\geq$}500, pool sizes of about 10 to 20 are generally satisfactory from a statistical viewpoint. The methodological advantages and disadvantages of more complicated pooling strategies involving repeated testing of units are discussed.},
  langid = {english},
  keywords = {Biometry,disease,pooling,prevalence},
  file = {C\:\\Users\\32498\\Zotero\\storage\\C8GS5VLA\\Abel et al. - 1999 - Estimating the prevalence of infectious agents usi.pdf}
}

