@article{sartoriusPrevalenceIntensitySoiltransmitted2020,
  title = {Prevalence and Intensity of Soil-Transmitted Helminth Infections of Children in Sub-{{Saharan Africa}}, 2000\textendash 18: A Geospatial Analysis},
