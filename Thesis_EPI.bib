  title = {Prevalence of Bovine Herpesvirus-1 in the {{Belgian}} Cattle Population},
  author = {Boelaert, F. and Biront, P. and Soumare, B. and Dispas, M. and Vanopdenbosch, E. and Vermeersch, J. P. and Raskin, A. and Dufey, J. and Berkvens, D. and Kerkhofs, P.},
  year = {2000},
