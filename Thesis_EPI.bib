@article{florComparisonBayesianFrequentist2020,
  title = {Comparison of {{Bayesian}} and Frequentist Methods for Prevalence Estimation under Misclassification},
  author = {Flor, Matthias and Wei{\ss}, Michael and Selhorst, Thomas and {M{\"u}ller-Graf}, Christine and Greiner, Matthias},
