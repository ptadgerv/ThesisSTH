@article{denwoodDistributionPathogenicNematode2008,
  title = {The Distribution of the Pathogenic Nematode {{Nematodirus}} Battus in Lambs Is Zero-Inflated},
  author = {Denwood, M. J. and Stear, M. J. and Matthews, L. and Reid, S. W. J. and Toft, N. and Innocent, G. T.},
  year = {2008},
  month = sep,
